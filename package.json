{
  "name": "hepta-sa",
  "version": "1.0.0",
  "description": "Hepta sa custom theme",
  "main": "webpack.config.js",
  "private": true,
  "browserLists": [
    "defaults"
  ],
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production --progress",
    "watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch --mode development --progress",
    "clean": "rm -rf build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/qendrimvrella/hepta-sa.git"
  },
  "keywords": [
    "Hepta",
    "Wordpress"
  ],
  "author": "Kotoch",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/qendrimvrella/hepta-sa/issues"
  },
  "homepage": "https://gitlab.com/qendrimvrella/hepta-sa#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.1",
